<% provide(:title, "Dashboard") %>

<div class="row text-center">
  <h2>Donor Dashboard</h2>
  <hr>
  <div class="col-md-6">
    <h3>My Donor Profile</h3>
    <%= link_to "Edit My Profile", edit_donor_panel_path(id: "profile") %>
    <h4>Name: <%= @donor.name %></h4>
    <h4>Email: <%= @donor.email %></h4>
    <h4>Rest of Fields</h4>
  </div>
  <div class="col-md-6">
    <h3>My Donations</h3>
    <h4>To code innext milestone</h4>
  </div>
</div>
